{
  "version": "0.3.0",
  "license": "MIT",
  "description": "A tool for generating TypeScript types from database schemas",
  "repository": "https://github.com/austinChappell/db-schema-gen",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "db-schema-gen",
  "author": "Austin Chappell",
  "module": "dist/db-schema-gen.esm.js",
  "size-limit": [
    {
      "path": "dist/db-schema-gen.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/db-schema-gen.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/yargs": "^17.0.24",
    "husky": "^8.0.3",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "postgres": "^3.3.4",
    "yargs": "^17.7.2"
  }
}
